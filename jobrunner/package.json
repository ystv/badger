{
  "name": "badger-jobrunner",
  "version": "0.0.0",
  "packageManager": "yarn@4.4.1",
  "type": "commonjs",
  "scripts": {
    "dev": "tsx watch --clear-screen=false src/index.ts",
    "prettify": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "build": "node esbuild.config.mjs",
    "lint": "eslint src/**/*.ts",
    "test": "vitest run",
    "test:integration": "TEST_INTEGRATION=true vitest run --config ./vitest.config-integration.mts --no-file-parallelism --silent=false"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.474.0",
    "@aws-sdk/lib-storage": "^3.428.0",
    "@badger/feature-flags": "workspace:*",
    "@badger/prisma": "workspace:*",
    "@badger/testing": "workspace:*",
    "@googleapis/drive": "^8.0.0",
    "@sentry/node": "^8.4.0",
    "@types/dotenv-flow": "^3.2.0",
    "@types/lodash": "^4.14.199",
    "dotenv-flow": "^4.0.0",
    "easydl": "^1.1.1",
    "got": "^13.0.0",
    "lodash": "^4.17.21",
    "loglevel": "^1.8.1",
    "loglevel-plugin-prefix": "^0.8.4",
    "p-event": "^6.0.0",
    "s3-readstream": "^2.0.1",
    "ts-expect": "^1.3.0"
  },
  "devDependencies": {
    "@sentry/esbuild-plugin": "^2.17.0",
    "@tsconfig/node18": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "esbuild": "^0.24.0",
    "eslint": "^8",
    "glob": "^11.0.0",
    "prettier": "^3.0.3",
    "tsx": "^4.16.2",
    "typescript": "^5.1.6",
    "vitest": "^2.0.0"
  }
}
